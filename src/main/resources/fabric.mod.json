{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",

  "name": "${name}",
  "description": "carpet addition made for vulpeus.",
  "authors": [
    "VulpeusServer"
  ],
  "contact": {
    "homepage": "https://vulpeus.com",
    "issues": "https://github.com/Vulpeus-Server/vulpeus-carpet/issues",
    "sources": "https://github.com/Vulpeus-Server/vulpeus-carpet"
  },

  "icon": "assets/vulpeus_carpet/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.vulpeus.vulpeus_carpet.VulpeusCarpetExtension"
    ]
  },
  "mixins": [
    "vulpeus_carpet.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.25",
    "carpet": "${carpet_dependency}",
